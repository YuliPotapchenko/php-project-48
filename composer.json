{
    "name": "hexlet/code",
    "type": "library",
    "license": "MIT",
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "files": [
            "src/Differ.php",
            "src/Parsers.php",
            "src/Format.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Json.php",
            "src/Formatters/Plain.php"
        ],
        "psr-4": {
            "Gendiff\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "authors": [
        {
            "name": "julia"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^3.4",
        "lstrojny/functional-php": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "10",
        "squizlabs/php_codesniffer": "*"
    }
}
